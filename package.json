{
  "name": "gibran-dev",
  "version": "2.0.0",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": "24.x",
    "pnpm": "^10"
  },
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm typegen && next build",
    "start": "next start",
    "lint": "eslint --fix && prettier . --write",
    "typecheck": "tsc --pretty --noEmit",
    "typegen": "scripts/typegen.sh",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/react": "^11.14.0",
    "devicon": "^2.17.0",
    "framer-motion": "^12.23.26",
    "lodash": "^4.17.21",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "qs": "^6.14.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-icons": "^5.5.0",
    "react-intl": "^8.0.4",
    "react-parallax-tilt": "^1.7.315",
    "react-player": "^2.11.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.30.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20",
    "@types/qs": "^6.14.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "type-fest": "^5.3.1",
    "typescript": "^5"
  }
}
