{
  "name": "gibran-dev",
  "version": "2.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix && prettier . --write",
    "typecheck": "tsc --pretty --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "^9 || ^10"
  }
}
